#!/usr/bin/env python3.6
"""
Window application to show weather and weather forecast.
Gets address from user and show result in table
base on superool.weather.py
"""
import sys
from PyQt5 import QtWidgets

import supertool.weather_gui.weather_app as wa

if __name__ == '__main__':
    app = QtWidgets.QApplication(sys.argv)
    window = wa.MainApp()
    window.show()
    sys.exit(app.exec_())