#!/usr/bin/env python3.6
"""
Execute some functionality from common calculator.(+/*^-)
And logically handle all actions with buttons.
Window application
"""
import sys
from PyQt5 import QtWidgets

import supertool.calculator as calc

if __name__ == '__main__':
    app = QtWidgets.QApplication(sys.argv)
    window = calc.MainApplication()
    window.show()
    sys.exit(app.exec_())